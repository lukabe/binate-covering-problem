<Application x:Class="BinateCoveringProblem.App.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:BinateCoveringProblem.App">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>
                    <local:Bootstrapper x:Key="Bootstrapper" />
                    <FontFamily x:Key="FontAwesomeBrands">/Fonts/fa-brands.otf#Font Awesome 5 Brands Regular</FontFamily>
                    <FontFamily x:Key="FontAwesomeRegular">/Fonts/fa-regular.otf#Font Awesome 5 Free Regular</FontFamily>
                    <FontFamily x:Key="FontAwesomeSolid">/Fonts/fa-solid.otf#Font Awesome 5 Free Solid</FontFamily>
                </ResourceDictionary>
                <ResourceDictionary Source="Styles/ButtonStyle.xaml" />
                <ResourceDictionary Source="Styles/TextBoxStyle.xaml" />
                <ResourceDictionary Source="Styles/DataGridStyle.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
